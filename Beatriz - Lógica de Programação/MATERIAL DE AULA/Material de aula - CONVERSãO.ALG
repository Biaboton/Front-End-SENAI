algoritmo "Conversão"
var
cotacao, dolar, resultado: real
inicio
      escreval ("Digite a cotação atual do dólar")
      leia (cotacao)
      escreval ("Digite a quantidade de dólares a ser convertidos: ")
      leia (dolar)
      resultado <- dolar * cotacao
      escreval ("o valor convertido é de", resultado)

fimalgoritmo

