algoritmo "Atividade3 - Valores"
var
   numero1: inteiro
   numero2: inteiro
inicio
      escreva("Digite um número:")
 leia (numero1)
 escreva ("Digite o segundo número:")
 leia (numero2)
 se (numero1=numero2)entao
 escreva ("Números Iguais")
 se (numero1>numero2)entao
 escreva ("Primeiro número é maior")
 fimSe
 fimSe
 
fimalgoritmo